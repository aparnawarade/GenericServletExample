<?xml version="1.0" encoding="UTF-8"?>

<web-app>

     <servlet>
        <servlet-name>MyFirstGenericServlet</servlet-name>
        <servlet-class>MyFirstGenericServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyFirstGenericServlet</servlet-name>
        <url-pattern>/genericServletExample/MyFirstGenericServlet</url-pattern>
    </servlet-mapping>
    
     <servlet>
        <servlet-name>secondServlet</servlet-name>
        <servlet-class>secondServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>secondServlet</servlet-name>
        <url-pattern>/genericServletExample/secondServlet</url-pattern>
    </servlet-mapping>
  
</web-app>